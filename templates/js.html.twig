<script type="text/javascript">
    $(document).ready(function(){
       global.settings.url = '{{ global_url }}';
       global.init(); 
        {# $('#modal').on('hide.bs.modal', function(){
            $("#ui-datepicker-div").remove();

        }); #}
    });

</script>

{% set success_messages =  app.session.flashbag.get('success_messages') %}
{% if success_messages %}
    <script type="text/javascript">
        var msg = ''; 
        {% for success_message in  success_messages %}
            msg+= "{{success_message}}" + "\n";
        {% endfor %}
        $.toaster({ message : msg, title : '', priority : 'success' });
    </script>
{% endif %}

{% set error_messages =  app.session.flashbag.get('error_messages') %}
{% if error_messages %}
      <script type="text/javascript">
        var msg = ''; 
        {% for error_message in  error_messages %}
                msg+= "{{error_message}}" + "\n";
        {% endfor %}
        $.toaster({ message : msg, title : '', priority : 'danger' });
    </script>
{% endif %}

{% if  javascripts is defined %}
    {% for script in javascripts %}
        <script src="{{ script }}?v={{ assets_version }}"></script>
    {% endfor  %}
{% endif %}



